<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <changeSet id="ksen_lpr-foreign-keys" author="alubbers">
        <addForeignKeyConstraint constraintName="FK1BE1597B1D2EFA44" baseTableName="KSEN_LPR" baseColumnNames="RELATION_TYPE_ID" referencedTableName="KSEN_LPR_TYPE" referencedColumnNames="TYPE_KEY" />
        <addForeignKeyConstraint constraintName="FK1BE1597B61E975F6" baseTableName="KSEN_LPR" baseColumnNames="RELATION_STATE_ID" referencedTableName="KSEN_COMM_STATE" referencedColumnNames="ID" />
    </changeSet>

    <changeSet id="ksen_lpr_attr-constraints" author="alubbers">
        <addForeignKeyConstraint constraintName="FKDF4BE635DC3CD520" baseTableName="KSEN_LPR_ATTR" baseColumnNames="OWNER" referencedTableName="KSEN_LPR" referencedColumnNames="ID" />
    </changeSet>
    
    <changeSet id="ksen_lpr_roster-constraints" author="alubbers">
	    <addNotNullConstraint tableName="KSEN_LPR_ROSTER" columnName="ID" columnDataType="java.sql.Types.VARCHAR(255)" />
	    <addPrimaryKey tableName="KSEN_LPR_ROSTER" columnNames="ID" constraintName="KSEN_LPR_ROSTER_PK"/>
	    <addForeignKeyConstraint constraintName="KSEN_LPR_ROSTER_FK1" baseTableName="KSEN_LPR_ROSTER" baseColumnNames="RT_DESCR_ID" referencedTableName="KSEN_RICH_TEXT_T" referencedColumnNames="ID" />
    </changeSet>
    
    <changeSet id="ksen_lprroster_lui_reltn-constraints" author="alubbers">
	    <addForeignKeyConstraint constraintName="KSEN_LPRROSTER_LUI_RELTN_FK1" baseTableName="KSEN_LPRROSTER_LUI_RELTN" baseColumnNames="LPRROSTER_ID" referencedTableName="KSEN_LPR_ROSTER" referencedColumnNames="ID" />
	    <addForeignKeyConstraint constraintName="KSEN_LPRROSTER_LUI_RELTN_FK2" baseTableName="KSEN_LPRROSTER_LUI_RELTN" baseColumnNames="LUI_ID" referencedTableName="KSEN_LUI" referencedColumnNames="ID" />
    </changeSet>

    <changeSet id="ksen_lpr_roster_entry-constraints" author="alubbers">
        <addPrimaryKey tableName="KSEN_LPR_ROSTER_ENTRY" columnNames="ID" constraintName="KSEN_LPR_ROSTER_ENTRY_PK"/>
	    <addForeignKeyConstraint constraintName="KSEN_LPR_ROSTER_ENTRY_FK1" baseTableName="KSEN_LPR_ROSTER_ENTRY" baseColumnNames="LPRROSTER_ID" referencedTableName="KSEN_LPR_ROSTER" referencedColumnNames="ID" />
	    <addForeignKeyConstraint constraintName="KSEN_LPR_ROSTER_ENTRY_FK2" baseTableName="KSEN_LPR_ROSTER_ENTRY" baseColumnNames="LPR_ID" referencedTableName="KSEN_LPR" referencedColumnNames="ID" />
    </changeSet>

</databaseChangeLog>
